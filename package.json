    "@project-serum/anchor": "^0.20.1",
    "@solana/wallet-adapter-wallets": "^0.16.1",
    "@solana/web3.js": "^1.44.1",
    "@types/node": "^17.0.14",
    "@types/node-fetch": "^2.6.2",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vueuse/core": "^7.5.5",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-wallets": "^0.15 | ^0.16"
